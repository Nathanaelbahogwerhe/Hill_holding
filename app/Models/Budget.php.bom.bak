<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;

class Budget extends Model
{
    use HasFactory;

    protected $fillable = [
        'title',
        'amount',
        'description',
        'start_date',
        'end_date',
        'filiale_id',
        'status',
    ];

    public function filiale()
    {
        return $this->belongsTo(Filiale::class);
    }
}







